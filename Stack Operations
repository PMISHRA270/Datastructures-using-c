#include<stdio.h>
#include<stdlib.h>
#define MAX 10
int top=-1;
int overflow()
{
    if(top==MAX-1)
    return 1;
    return 0;
}
int underflow()
{
    if(top==-1)
    return 1;
    return 0;
}
void push(int *s,int ele)
{
    if(!overflow())
    s[++top]=ele;
    else
    printf("stack overflow\n");
}
int pop(int *s)
{
    if(!underflow())
    printf("the deleted element is:%d\n",s[top--]);
    else
    printf("stack underflow\n");
}
void display(int *s)
{
    if(!underflow())
    {
        printf("the elements of the stack are:\n");
        for(i=0;i<=top;i++)
        printf("%d\t",s[i]);
     }
     else
      printf("stack is empty\n");
}
